<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@drawable/pic_menu"
              android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/CjjCsSettingActivity_btn_Gprs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="读写GPRS参数"
            android:textColor="@color/white"/>

        <Button
            android:id="@+id/CjjCsSettingActivity_btn_LoRa"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="读写LoRa参数"
            android:textColor="@color/white"/>

        <Button
            android:id="@+id/CjjCsSettingActivity_btn_CjjSetting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="读写采集机参数"
            android:textColor="@color/white"/>


    </LinearLayout>

</LinearLayout>
